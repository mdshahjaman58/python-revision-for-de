   "source": [
    "# Function to find the sum of a list (without using sum())\n",
    "def list_sum(numbers):\n",
    "    total = 0\n",
    "    for num in numbers:\n",
    "        total += num\n",
    "    return total\n",
    "\n",
    "print(list_sum([1, 2, 3, 4, 5]))\n",
    "\n"
   ]
  }
