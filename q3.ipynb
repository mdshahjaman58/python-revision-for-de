    "# 3. Union, Intersection, Difference, Symmetric Difference\n",
    "a = {1, 2, 3, 4}\n",
    "b = {3, 4, 5, 6}\n",
    "\n",
    "print(\"Union:\", a | b)\n",
    "print(\"Intersection:\", a & b)\n",
    "print(\"Difference (a-b):\", a - b)\n",
    "print(\"Symmetric Difference:\", a ^ b)\n",
    "v"
   ]
